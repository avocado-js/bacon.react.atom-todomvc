{
  "name": "bral-todomvc",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "export NODE_ENV=production && browserify src/todomvc.js -t babelify -t envify -g uglifyify > public/app.js",
    "lint": "eslint src test",
    "prepublish": "npm run lint && npm run build",
    "watch": "watchify src/todomvc.js -v -t [ babelify --sourceMaps=inline ] -o public/app.js"
  },
  "dependencies": {
    "bacon.atom": "^3.1.1",
    "bacon.react.html": "^3.1.1",
    "baconjs": "^0.7.84",
    "partial.lenses": "^3.6.0",
    "ramda": "^0.21.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "todomvc-app-css": "^2.0.6"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.8.0",
    "babel-plugin-transform-react-constant-elements": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "envify": "^3.4.0",
    "eslint": "^2.10.2",
    "eslint-plugin-react": "^5.1.1",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0"
  }
}
