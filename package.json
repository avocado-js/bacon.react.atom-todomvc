{
  "name": "atomi-todomvc",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "export NODE_ENV=production && browserify src/todomvc.js -t babelify -t envify -g uglifyify > public/app.js",
    "lint": "eslint src test",
    "prepublish": "npm run lint && npm run build",
    "watch": "watchify src/todomvc.js -v -t [ babelify --sourceMaps=inline ] -o public/app.js"
  },
  "dependencies": {
    "bacon.atom": "^2.0.1",
    "bacon.react.html": "^1.4.0",
    "baconjs": "^0.7.83",
    "partial.lenses": "^0.7.0",
    "ramda": "^0.19.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "todomvc-app-css": "^2.0.4"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.5.1",
    "babel-eslint": "^5.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.5.2",
    "babel-plugin-transform-react-constant-elements": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "envify": "^3.4.0",
    "eslint": "^2.2.0",
    "eslint-plugin-react": "^4.1.0",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0"
  }
}
